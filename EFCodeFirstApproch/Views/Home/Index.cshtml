@model IEnumerable<EFCodeFirstApproch.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>First Code</h2>
<table border="2">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Gender</td>
        <td>Age</td>
    </tr>
    @{ 
        foreach(var item in Model)
            {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Age</td>
        </tr>
            }
    }
</table>
